{
    "get": {
        "tags": [
            "SCIM"
        ],
        "description": "Returns a paginated list of members bound to a organization with a SCIM Users GET Request. See here for details on setting up SCIM.",
        "operationId": "List an Organization's Members",
        "parameters": [
            {
                "name": "organization_slug",
                "in": "path",
                "required": true,
                "schema": {
                    "type": "string"
                }
            },
            {
                "$ref": "../../components/parameters/scim.json#/startIndex"
            },
            {
                "$ref": "../../components/parameters/scim.json#/filter"
            },
            {
                "$ref": "../../components/parameters/scim.json#/count"
            }
        ],
        "responses": {
            "200": {
                "description": "Success",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "items": {
                                "$ref": "../../components/schemas/scim/user_list.json"
                            }
                        },
                        "example": {
                            "schemas": [
                                "urn:ietf:params:scim:api:messages:2.0:ListResponse"
                            ],
                            "totalResults": 1,
                            "startIndex": 1,
                            "itemsPerPage": 1,
                            "Resources": [
                                {
                                    "schemas": [
                                        "urn:ietf:params:scim:schemas:core:2.0:User"
                                    ],
                                    "id": 102,
                                    "userName": "test.user@okta.local",
                                    "emails": [
                                        {
                                            "primary": true,
                                            "value": "test.user@okta.local",
                                            "type": "work"
                                        }
                                    ],
                                    "name": {
                                        "familyName": "N/A",
                                        "givenName": "N/A"
                                    },
                                    "active": true,
                                    "meta": {
                                        "resourceType": "User"
                                    }
                                }
                            ]
                        }
                    }
                }
            },
            "401": {
                "description": "Permission Denied"
            },
            "403": {
                "description": "Forbidden"
            },
            "404": {
                "description": "Not Found"
            }
        },
        "security": [
            {
                "auth_token": [
                    "member:read"
                ]
            }
        ]
    },
    "post": {
        "tags": [
            "SCIM"
        ],
        "description": "Create a new Organization Member via a SCIM POST Users Request.",
        "operationId": "Create a New Organization Member",
        "parameters": [
            {
                "name": "organization_slug",
                "in": "path",
                "required": true,
                "schema": {
                    "type": "string"
                }
            }
        ],
        "requestBody": {
            "content": {
                "application/json": {
                    "schema": {
                        "required": [
                            "name"
                        ],
                        "type": "object",
                        "properties": {
                            "schemas": {
                                "$ref": "../../components/schemas/scim/user.json#/definitions/schemas"
                            },
                            "emails": {
                                "$ref": "../../components/schemas/scim/user.json#/definitions/emails"
                            },
                            "userName": {
                                "$ref": "../../components/schemas/scim/user.json#/definitions/userName"
                            },
                            "active": {
                                "$ref": "../../components/schemas/scim/user.json#/definitions/active"
                            }
                        }
                    },
                    "example": {
                        "schemas": [
                            "urn:ietf:params:scim:schemas:core:2.0:User"
                        ],
                        "userName": "test.user@okta.local",
                        "name": {
                            "givenName": "Test",
                            "familyName": "User"
                        },
                        "emails": [
                            {
                                "primary": true,
                                "value": "test.user@okta.local",
                                "type": "work"
                            }
                        ],
                        "displayName": "Test User",
                        "locale": "en-US",
                        "externalId": "00ujl29u0le5T6Aj10h7",
                        "groups": [],
                        "active": true
                    }
                }
            },
            "required": true
        },
        "responses": {
            "201": {
                "description": "Success",
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "../../components/schemas/scim/user.json#User"
                        },
                        "example": {
                            "schemas": [
                                "urn:ietf:params:scim:schemas:core:2.0:User"
                            ],
                            "id": 242,
                            "userName": "test.user@okta.local",
                            "emails": [
                                {
                                    "primary": true,
                                    "value": "test.user@okta.local",
                                    "type": "work"
                                }
                            ],
                            "active": true,
                            "name": {
                                "familyName": "N/A",
                                "givenName": "N/A"
                            },
                            "meta": {
                                "resourceType": "User"
                            }
                        }
                    }
                }
            },
            "400": {
                "description": "Bad input"
            },
            "403": {
                "description": "Forbidden"
            },
            "409": {
                "description": "User already exists in the database."
            }
        },
        "security": [
            {
                "auth_token": [
                    "member:write"
                ]
            }
        ]
    }
}
